;;; Input

(define (_main)
  (println-non-void
   (begin
     (= y (begin (= x 20) (iadd x (begin (= x 22) x))))
     y)))

;;; ShrinkPass

(define (_main)
  (println-non-void
   (@let1 y (@let1 x 20 ((iadd x) (@let1 x 22 x))) y)))

;;; UniquifyPass

(define (_main)
  (println-non-void
   (@let1 y₁ (@let1 x₁ 20 ((iadd x₁) (@let1 x₂ 22 x₂))) y₁)))

;;; RevealGlobalPass

(define (_main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (((@primitive-function iadd 2) x₁) (@let1 x₂ 22 x₂)))
     y₁)))

;;; LiftLambdaPass

(define (_main)
  ((@primitive-function println-non-void 1)
   (@let1 y₁
       (@let1 x₁ 20
         (((@primitive-function iadd 2) x₁) (@let1 x₂ 22 x₂)))
     y₁)))

;;; UnnestOperandPass

(define (_main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 y₁
        (@let1 x₁ 20
          (@let1 _₂ (@primitive-function iadd 2)
            (@let1 _₃ (_₂ x₁) (@let1 x₂ 22 (_₃ x₂)))))
      (_₁ y₁))))

