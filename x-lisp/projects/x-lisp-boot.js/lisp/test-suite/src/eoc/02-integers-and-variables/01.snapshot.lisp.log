;;; Input

(define (_main)
  (println-non-void (begin (= x 4) (iadd 8 x))))

;;; ShrinkPass

(define (_main) (println-non-void (@let1 x 4 ((iadd 8) x))))

;;; UniquifyPass

(define (_main)
  (println-non-void (@let1 x₁ 4 ((iadd 8) x₁))))

;;; RevealGlobalPass

(define (_main)
  ((@primitive-function println-non-void 1)
   (@let1 x₁ 4 (((@primitive-function iadd 2) 8) x₁))))

;;; LiftLambdaPass

(define (_main)
  ((@primitive-function println-non-void 1)
   (@let1 x₁ 4 (((@primitive-function iadd 2) 8) x₁))))

;;; UnnestOperandPass

(define (_main)
  (@let1 _₁ (@primitive-function println-non-void 1)
    (@let1 x₁ 4
      (@let1 _₂ (@primitive-function iadd 2)
        (@let1 _₃ 8
          (@let1 _₄ (_₂ _₃) (@let1 _₅ (_₄ x₁) (_₁ _₅))))))))

