(define-function _main
  (block entry
    (goto print-bool))

  (block print-bool
    (= _ (literal #t))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal #f))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (goto print-int))

  (block print-int
    (= _ (literal 0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal 1))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal -1))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (goto print-float))

  (block print-float
    (= _ (literal 0.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal 1.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal -1.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (return)))
