(define-function _main
  (block entry
    (= one (literal 1))
    (= zero (literal 0))
    (= x (call (@primitive-function zero? 1) one))
    (= x.1 (call (@primitive-function not 1) x))
    (assert x.1)
    (= y (call (@primitive-function zero? 1) zero))
    (assert y)
    (return)))

(define-function zero?
  (block entry
    (= x (argument 0))
    (= zero (literal 0))
    (= b (call (@primitive-function equal? 2) x zero))
    (branch b then else))

  (block then
    (= true (literal #t))
    (return true))

  (block else
    (= false (literal #f))
    (return false)))
