(define-function _main
  (block entry
    ;; bool
    (= _ (literal #t))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal #f))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))

    ;; int
    (= _ (literal 0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal 1))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal -1))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))

    ;; float
    (= _ (literal 0.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal 1.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (= _ (literal -1.0))
    (call (@primitive-function print 1) _)
    (call (@primitive-function newline 0))
    (return)))
