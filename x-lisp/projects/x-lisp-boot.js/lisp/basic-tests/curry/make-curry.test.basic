(define-function _main
  (block entry
    (= zero (literal 0))
    (= one (literal 1))
    (= two (literal 2))
    (= iadd-fn (literal (@function iadd 2)))
    (= iadd1 (call (@primitive-function make-curry 3) iadd-fn one one))
    (call (@primitive-function curry-put! 3) zero one iadd1)
    (= x (literal 5))
    (= result (apply iadd1 x))
    (= expected (literal 6))
    (= ok (call (@primitive-function equal? 2) result expected))
    (assert ok)
    (return)))
