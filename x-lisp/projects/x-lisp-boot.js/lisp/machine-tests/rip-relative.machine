(export start)

(define-data message (string "Hello, World!\n"))
(define-data message.length (dq 14))

(define-code start
  (block entry
    (leaq (label-deref (label message)) (reg rsi))
    (nop)
    (nop)
    (nop)
    (leaq (label-deref (label message)) (reg rdi))
    (movq (imm 60) (reg rax))
    (subq (reg rsi) (reg rdi))
    (syscall)))
