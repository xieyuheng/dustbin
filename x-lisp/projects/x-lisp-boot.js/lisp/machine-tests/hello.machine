(export start)

(define-data message (string "Hello, World!\n"))
(define-data message.length (dq 14))

(define-code start
  (block entry
    (movq (imm 1) (reg rax))
    (movq (imm 1) (reg rdi))
    (movq (label-imm (label message)) (reg rsi))
    (movq (imm 14) (reg rdx))
    (syscall)
    (movq (imm 60) (reg rax))
    (movq (imm 0) (reg rdi))
    (syscall)))
